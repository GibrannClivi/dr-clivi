# Modify this file and rename it to ".env"

# Google Cloud Configuration
GOOGLE_GENAI_USE_VERTEXAI=1
GOOGLE_CLOUD_PROJECT=<YOUR_PROJECT_NAME>
GOOGLE_CLOUD_LOCATION=<YOUR_PROJECT_LOCATION>
GOOGLE_CLOUD_STORAGE_BUCKET=<YOUR_STORAGE_BUCKET>

# WhatsApp/Twilio Configuration
TWILIO_ACCOUNT_SID=<YOUR_TWILIO_ACCOUNT_SID>
TWILIO_AUTH_TOKEN=<YOUR_TWILIO_AUTH_TOKEN>
TWILIO_WHATSAPP_NUMBER=<YOUR_TWILIO_WHATSAPP_NUMBER>

# A2A Configuration for agent communication
A2A_REGISTRY_URL=<YOUR_A2A_REGISTRY_URL>
A2A_AGENT_ID=<YOUR_AGENT_ID>
A2A_SECRET_KEY=<YOUR_A2A_SECRET_KEY>

# Clivi API Configuration
CLIVI_API_BASE_URL=<YOUR_CLIVI_API_URL>
CLIVI_API_KEY=<YOUR_CLIVI_API_KEY>

# Database Configuration (if needed)
DATABASE_URL=<YOUR_DATABASE_URL>

# Model Configuration
DEFAULT_MODEL=gemini-2.5-flash
COORDINATOR_MODEL=gemini-2.5-pro

# Logging Level
LOG_LEVEL=INFO
